<%= flash[:notice] %>
<table>
  <thead>
    <tr>
      <th align="left">Code</th>
      <th align="left">Name</th>
      <th align="left">Category</th>
	</tr>
  </thead>
  <tbody>
    <% @nominals.each do |nominal| %>
      <tr>
        <td><%= nominal.id %></td>
        <td><%= nominal.name %></td>
        <td><%= nominal.category %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div>
<h3>Import a CSV File</h3>
  <%= form_tag import_nominals_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
</div>